{
  "manifest_version": 3,
  "name": "LinkedIn AI Reply",
  "version": "1.0.0",
  "description": "AI-powered reply suggestions for LinkedIn messages",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["src/content.tsx"]
    }
  ]
}
